{
  "name": "@card-architect/utils",
  "version": "0.1.0",
  "type": "module",
  "description": "Shared utilities for character card format handlers",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./binary": {
      "import": "./dist/binary.js",
      "types": "./dist/binary.d.ts"
    },
    "./base64": {
      "import": "./dist/base64.js",
      "types": "./dist/base64.d.ts"
    },
    "./zip": {
      "import": "./dist/zip.js",
      "types": "./dist/zip.d.ts"
    },
    "./uri": {
      "import": "./dist/uri.js",
      "types": "./dist/uri.d.ts"
    },
    "./macros": {
      "import": "./dist/macros.js",
      "types": "./dist/macros.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@card-architect/schemas": "*"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
